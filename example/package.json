{
  "name": "three-quick-vr-example",
  "version": "0.0.0",
  "description": "three-quickvr demostration",
  "scripts": {
    "start": "npm run dev",
    "prebuild": "npm run clean",
    "build": "webpack -p",
    "clean": "rimraf ./dist",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d ./dist",
    "dev": "webpack-dev-server --open"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "google",
    "globals": {
      "document": "true",
      "window": "true"
    },
    "rules": {
      "max-len": [
        1,
        120
      ],
      "require-jsdoc": [
        0
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "comment-empty-line-before": null,
      "rule-empty-line-before": null,
      "rule-nested-empty-line-before": null,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "css-loader": "^0.27.2",
    "cssnano": "^3.10.0",
    "eslint": "^3.17.1",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.10.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "normalize.css": "^5.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.6.1",
    "robotstxt-webpack-plugin": "^1.0.1",
    "style-loader": "^0.13.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-glsl-loader": "^1.0.1"
  },
  "dependencies": {
    "three": "^0.84.0",
    "three-quickvr": "^0.1.0",
    "three-vrcontrols-module": "^1.1.1",
    "three-vreffect-module": "^1.0.1",
    "webvr-polyfill": "^0.9.26",
    "webvr-ui": "^0.9.4"
  }
}
